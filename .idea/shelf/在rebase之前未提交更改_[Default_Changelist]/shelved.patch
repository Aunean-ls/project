+        option3 = {
+
+        title:{text:'各个小时的通话总次数',
+        subtext:'2017年7月',
+        textStyle:{color:'red',},
+        x:'center',},
+
+        xAxis:{
+        type:'category',
+        data:[{% for i in time_A %}"{{i}}"+"H",{%  endfor%}]
+        },
+        yAxis:{
+        type:'value'
+        },
+            tooltip: {
+        trigger: 'axis'
+    },
+    toolbox: {
+        show: true,
+        feature: {
+          saveAsImage: {}
+        }
+    },
+        series:[{
+        type:'line',
+        data:[{% for i in count_A %}{{i}},{% endfor %}]}]
